#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/XG/vscodeProjects/somedays/node_modules/.pnpm/vitepress@1.2.3_@algolia+client-search@4.24.0_@types+react@18.3.3_async-validator@4.2.5_less@_ydh2rbmw4ds5ciokghp42ikzq4/node_modules/vitepress/bin/node_modules:/Users/XG/vscodeProjects/somedays/node_modules/.pnpm/vitepress@1.2.3_@algolia+client-search@4.24.0_@types+react@18.3.3_async-validator@4.2.5_less@_ydh2rbmw4ds5ciokghp42ikzq4/node_modules/vitepress/node_modules:/Users/XG/vscodeProjects/somedays/node_modules/.pnpm/vitepress@1.2.3_@algolia+client-search@4.24.0_@types+react@18.3.3_async-validator@4.2.5_less@_ydh2rbmw4ds5ciokghp42ikzq4/node_modules:/Users/XG/vscodeProjects/somedays/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/XG/vscodeProjects/somedays/node_modules/.pnpm/vitepress@1.2.3_@algolia+client-search@4.24.0_@types+react@18.3.3_async-validator@4.2.5_less@_ydh2rbmw4ds5ciokghp42ikzq4/node_modules/vitepress/bin/node_modules:/Users/XG/vscodeProjects/somedays/node_modules/.pnpm/vitepress@1.2.3_@algolia+client-search@4.24.0_@types+react@18.3.3_async-validator@4.2.5_less@_ydh2rbmw4ds5ciokghp42ikzq4/node_modules/vitepress/node_modules:/Users/XG/vscodeProjects/somedays/node_modules/.pnpm/vitepress@1.2.3_@algolia+client-search@4.24.0_@types+react@18.3.3_async-validator@4.2.5_less@_ydh2rbmw4ds5ciokghp42ikzq4/node_modules:/Users/XG/vscodeProjects/somedays/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vitepress/bin/vitepress.js" "$@"
else
  exec node  "$basedir/../vitepress/bin/vitepress.js" "$@"
fi
